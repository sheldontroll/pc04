@model List<Fail>


@if (Model.Count()==0)
{
    <p class="text-center"> No hay imagenes para mostrar </p>
}else{
    @foreach (var item in Model)
    {
        <div class="col-2 text-center">
            <h2>@item.titulo</h2>
            <img src="@item.gif" class="img-fluid">
        </div>
    }
}
